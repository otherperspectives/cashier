{
    "name": "bgultekin/cashier-fastspring",
    "description": "Cashier Fastspring is a cashier-like laravel package which provides an interface to Fastspring subscription and payment services.",
    "keywords": ["laravel", "fastspring", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Gultekin",
            "email": "bilal@gultekin.me"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "symfony/http-kernel": "^6.1"
    },
    "require-dev": {
        "illuminate/http": "^9.0",
        "illuminate/routing": "^9.0",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "~3"
    },
    "autoload": {
        "psr-4": {
            "Bgultekin\\CashierFastspring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bgultekin\\CashierFastspring\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.0-dev"
        },
        "laravel": {
            "providers": [
                "Bgultekin\\CashierFastspring\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html=coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
